<h1>Parties#index</h1>
<p>Find me in app/views/parties/index.html.erb</p>
<h2>Your Playlists:</h2>

<ul id="parties">
<% @parties.each do |party| %>
		<% if party["code"] %>
			<a href="/<%=party["code"]%>"><%=party["name"]%></a><br>
		<%end%>
<% end %>
</ul>

<%= link_to 'create a new party', new_party_path %>